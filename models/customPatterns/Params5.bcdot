agent U: a, b
agent V: a, b
agent W: a, b
agent X: a, b
agent Y: a, b
agent Z: a, b

init 20 (u: U, v: V, w: W, x: X, y: Y): u//0, v//0, w//0, x//0, y//0

rule osc(u: U, v: V, w: W, x: X, y: Y): u.a//0, v.a//0, w.a//0, x.a//0, y.b//0, x.b//0 <=> u.a+v.a,  w.a+x.a, x.b+y.b @1.0, 1.0

terminate iterations = 1000	